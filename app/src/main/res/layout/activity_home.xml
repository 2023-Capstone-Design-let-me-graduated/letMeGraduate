<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/white">
        <TextView
            android:id="@+id/id_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:text="∘ 졸업"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="180dp"
                        android:layout_height="40dp"
                        android:text="졸업 여부"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/graduationstatus"
                        android:layout_width="181dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:text="∘ 학점"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="졸업 학점"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="취득 학점"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="남은 학점"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/graduationCredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="140"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/totalcredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/remainingcredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="24dp"
                android:background="@drawable/layout_stroke"
                tools:ignore="UselessParent">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="360dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="충족 여부"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:text="이수구분"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="최소\n 이수학점"
                        tools:ignore="HardcodedText,TextSizeCheck" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="78dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="취득 학점"
                        tools:ignore="HardcodedText,TextSizeCheck" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="최소 이수학점까지 남은 학점"
                        tools:ignore="HardcodedText,TextSizeCheck" />
                </TableRow>
            </TableLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:text="@string/liberalarts"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/minimumLACompletionCredit"
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="30"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/leberalartisCredit"
                        android:layout_width="78dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/remainingLACredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:text="@string/major"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/minimumMCompletionCredit"
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="72"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/majorCredit"
                        android:layout_width="78dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/remainingMajorCredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:text="전공필수"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="TooManyViews,UselessParent">
                    <TextView
                        android:id="@+id/minimumMNCompletionCredit"
                        android:layout_width="80dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="19"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/majorneedCredit"
                        android:layout_width="78dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/remainingMNCredit"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:text="∘ 영어졸업인증제"
            android:textColor="@color/black"
            android:textSize="19sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="구분"
                        android:gravity="center"
                        tools:ignore="HardcodedText,TooManyViews" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="#E3DEDE"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="자격기준 점수조건"
                        tools:ignore="HardcodedText,TextSizeCheck" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    android:background="#E3DEDE"
                    tools:ignore="UselessParent">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:text="영어졸업인증 신청"
                        tools:ignore="HardcodedText,TextSizeCheck" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/gray"
                    tools:ignore="UselessParent">
                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:text="충족 여부"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/scoreSatisfacionStatus"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText" />
                </TableRow>
            </TableLayout>
            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_stroke">
                <TableRow
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginEnd="2dp"
                    android:layout_marginBottom="2dp"
                    android:background="@color/white"
                    tools:ignore="UselessParent">
                    <TextView
                        android:id="@+id/applicationStatus"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center"
                        tools:ignore="HardcodedText,RtlHardcoded" />
                </TableRow>
            </TableLayout>
        </LinearLayout>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/applicationBtn"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:paddingTop="0dp"
            android:paddingStart="0dp"
            android:paddingEnd="0dp"
            android:text="영어졸업인증 신청 여부 확인  >>"
            android:textStyle="bold"
            android:textColor="@color/black"
            app:strokeColor="@color/white"
            tools:ignore="HardcodedText,RtlSymmetry,TextSizeCheck" />
    </LinearLayout>
</layout>